<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#ffffff"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-28dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/img_call_hall"
        android:layout_width="@dimen/x373"
        android:layout_height="@dimen/x546"
        android:src="@color/aqua"
        android:layout_marginTop="@dimen/x136"
        android:layout_marginLeft="@dimen/x512"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:id="@+id/img_call"
        android:layout_width="@dimen/x182"
        android:layout_height="@dimen/x60"
        android:src="@mipmap/rounded"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/x690"
        android:layout_marginLeft="@dimen/x607"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="呼叫"
        android:textColor="#FFFFFF"
        android:textSize="@dimen/x42"
        android:layout_marginLeft="@dimen/x649"
        android:layout_marginTop="@dimen/x694"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/img_police_office"
        android:layout_width="@dimen/x45"
        android:layout_height="@dimen/x44"
        android:src="@mipmap/rectangle"
        android:layout_marginTop="@dimen/x315"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/x1022"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/img_police_office_select"
        android:layout_width="@dimen/x28"
        android:layout_height="@dimen/x28"
        android:src="@mipmap/select"
        android:visibility="gone"
        android:layout_marginTop="@dimen/x320"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/x1028"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#474747"
        android:text="民警办公室1"
        android:layout_marginTop="@dimen/x325"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/img_police_office"
        tools:ignore="MissingConstraints" />




    <ImageView
        android:id="@+id/img_police_office_b"
        android:layout_width="@dimen/x45"
        android:layout_height="@dimen/x44"
        android:src="@mipmap/rectangle"
        android:layout_marginTop="@dimen/x391"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/x1022"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/img_police_office_select_b"
        android:layout_width="@dimen/x28"
        android:layout_height="@dimen/x28"
        android:visibility="gone"
        android:src="@mipmap/select"
        android:layout_marginTop="@dimen/x396"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/x1028"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#474747"
        android:text="民警办公室2"
        android:layout_marginTop="@dimen/x402"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/img_police_office"
        tools:ignore="MissingConstraints" />




    <ImageView
        android:id="@+id/img_command"
        android:layout_width="@dimen/x45"
        android:layout_height="@dimen/x44"
        android:src="@mipmap/rectangle"
        android:layout_marginTop="@dimen/x464"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/x1022"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/img_command_select"
        android:layout_width="@dimen/x28"
        android:layout_height="@dimen/x28"
        android:visibility="gone"
        android:src="@mipmap/select"
        android:layout_marginTop="@dimen/x470"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/x1028"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#474747"
        android:text="指挥大厅"
        android:layout_marginTop="@dimen/x476"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/img_police_office"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>
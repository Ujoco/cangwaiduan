<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context=".qfragment.Inspection.QFragmentMonitoringA">


    <ImageView
        android:id="@+id/img_monitoring"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x790"
        android:src="@color/blue"
        android:layout_marginLeft="@dimen/x4"
        android:layout_marginTop="@dimen/x5"
        android:layout_marginRight="@dimen/x319"
        android:layout_marginBottom="@dimen/x5"
        android:overScrollMode="never"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_monitor"
        android:layout_width="@dimen/x300"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/x5"
        app:layout_constraintLeft_toRightOf="@+id/img_monitoring"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="@dimen/x5"
        android:layout_marginBottom="@dimen/x7"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>
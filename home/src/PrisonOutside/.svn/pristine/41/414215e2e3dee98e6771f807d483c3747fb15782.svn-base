<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    >


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="#F5F4F1"
        android:layout_marginLeft="@dimen/x55"
        android:layout_marginTop="@dimen/x35"
        android:layout_marginRight="@dimen/x533"
        android:layout_marginBottom="@dimen/x40"
        android:overScrollMode="never"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="#0DB8D1"
        android:layout_marginLeft="@dimen/x197"
        android:layout_marginTop="@dimen/x83"
        android:layout_marginRight="@dimen/x701"
        android:layout_marginBottom="@dimen/x40"

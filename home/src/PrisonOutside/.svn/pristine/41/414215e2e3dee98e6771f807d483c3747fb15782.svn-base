        android:layout_height="wrap_content"
        android:text="签到成功"
        android:textColor="@color/white"
        android:textSize="@dimen/x31"
        android:layout_marginTop="@dimen/x470"
        android:layout_marginRight="@dimen/x194"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


    <TextView
        android:layout_width="@dimen/x350"
        android:layout_height="wrap_content"
        android:text="姓名：李某某"
        android:textColor="#474747"
        android:textSize="@dimen/x26"
        android:layout_marginTop="@dimen/x552"
        android:layout_marginRight="@dimen/x84"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />



    <TextView
        android:layout_width="@dimen/x350"
        android:layout_height="wrap_content"
        android:text="编号：0101011"
        android:textColor="#474747"
        android:textSize="@dimen/x26"
        android:layout_marginTop="@dimen/x615"
        android:layout_marginRight="@dimen/x84"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


    <TextView
        android:layout_width="@dimen/x350"
        android:layout_height="wrap_content"
        android:text="时间：2019年11月01日 17:30"
        android:textColor="#474747"
        android:textSize="@dimen/x26"
        android:layout_marginTop="@dimen/x668"
        android:layout_marginRight="@dimen/x84"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>
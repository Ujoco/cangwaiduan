<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#E9E9E9"
    android:layout_width="@dimen/x913"
    android:layout_height="wrap_content">


    <ImageView
        android:layout_width="@dimen/x239"
        android:layout_height="@dimen/x155"
        android:src="@mipmap/header"
        android:layout_marginLeft="@dimen/x27"
        android:layout_marginTop="@dimen/x21"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="《中国地名大会》"
        android:textSize="@dimen/x30"
        android:textColor="#474747"
        android:layout_marginLeft="@dimen/x287"
        android:layout_marginTop="@dimen/x29"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />



    <TextView
        android:layout_width="@dimen/x582"
        android:layout_height="wrap_content"
        android:text="作为一档旨在弘扬民族文化的节目，《中国地名大会》将以地名知识为载体，从地理、历史、语言、文学、民俗等各个角度全方位展现中华大地"
        android:textSize="@dimen/x21"
        android:textColor="#474747"
        android:layout_marginLeft="@dimen/x287"
        android:layout_marginTop="@dimen/x68"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />
    
    <ImageView
        android:id="@+id/img_select"
        android:layout_width="@dimen/x38"
        android:layout_height="@dimen/x38"
        android:src="@mipmap/rectangle"
        android:layout_marginLeft="@dimen/x810"
        android:layout_marginTop="@dimen/x29"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/img_hook"
        android:layout_width="@dimen/x22"
        android:layout_height="@dimen/x22"
        android:src="@mipmap/select"
        android:layout_marginLeft="@dimen/x816"
        android:layout_marginTop="@dimen/x36"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />





</androidx.constraintlayout.widget.ConstraintLayout>
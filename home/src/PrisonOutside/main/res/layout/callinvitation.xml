<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#ffffff"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-28dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:layout_width="@dimen/x954"
        android:layout_height="@dimen/x526"
        android:src="@mipmap/rectangle_gray"
        android:layout_marginLeft="@dimen/x477"
        android:layout_marginTop="@dimen/x121"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />



    <ImageView
        android:layout_width="@dimen/x291"
        android:layout_height="@dimen/x369"
        android:src="@mipmap/a"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/x676"
        android:layout_marginTop="@dimen/x200"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:layout_width="@dimen/x70"
        android:layout_height="@dimen/x120"
        android:src="@mipmap/microphone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/x1040"
        android:layout_marginTop="@dimen/x448"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/img_answer"
    android:layout_width="@dimen/x171"
    android:layout_height="@dimen/x56"
    android:src="@mipmap/rounded"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    android:layout_marginLeft="@dimen/x868"
    android:layout_marginTop="@dimen/x682"
    tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="接听"
        android:textColor="#FFFFFF"
        android:textSize="@dimen/x40"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/x916"
        android:layout_marginTop="@dimen/x682"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>